{
  "name": "kosmos3",
  "version": "1.1.20",
  "description": "KosmoS3",
  "main": "main.js",
  "scripts": {
    "version": "echo $npm_package_version",
    "format": "prettier -w .",
    "clean:icons": "rimraf icons icon.png",
    "clean": "rimraf dist out && npm run clean:icons",
    "lint": "eslint --fix .",
    "build": "npm run fel && npm run cms",
    "all": "npm run clean && npm run build && npm run icons && npm run pkg",
    "serve": "webpack serve --mode development --open --config ./cms/webpack.config.js",
    "cms": "webpack --env production=true --config ./cms/webpack.config.js",
    "icons": "npm run clean:icons && .\"/node_modules/.bin/svg2png\" .\"/rocket.svg\" --output=.\"/icon.png\" && .\"/node_modules/.bin/electron-icon-builder\" --flatten --input=.\"/icon.png\" --output=.",
    "fel": "webpack --config ./fel/webpack.config.js",
    "pkg": "webpack --config ./pkg/webpack.config.js",
    "fdocs": "jsdoc -c ./.jsdoc.conf.fel.json",
    "cdocs": "jsdoc -c ./.jsdoc.conf.cms.json",
    "darwin": "npx electron-packager dist KosmoS3 --platform=darwin --arch=all --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "win32": "npx electron-packager dist KosmoS3 --platform=win32 --arch=x64 --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "linux": "npx electron-packager dist --platform=linux --arch=x64 --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "dmg:x64": "./node_modules/.bin/electron-installer-dmg --overwrite --title=KosmoS3 --icon=./icons/icon.icns ./out/kosmos3-darwin-x64/KosmoS3.app ./out/KosmoS3-x64",
    "dmg:arm64": "./node_modules/.bin/electron-installer-dmg --overwrite --title=KosmoS3 --icon=./icons/icon.icns ./out/kosmos3-darwin-arm64/KosmoS3.app ./out/KosmoS3-arm64",
    "deb": "./node_modules/.bin/electron-installer-debian --src out/kosmos3-linux-x64 --dest out --arch amd64",
    "msi": ".\"/node_modules/.bin/electron-installer-windows\" --src \"./out/KosmoS3-win32-x64\" --dest out",
    "mac": "npm run darwin && npm run dmg:x64 && npm run dmg:arm64",
    "win": "npm run win32 && npm run msi",
    "lin": "npm run linux && npm run deb",
    "macos-latest": "npm run all && npm run mac",
    "windows-latest": "npm run all && npm run win",
    "ubuntu-latest": "npm run all && npm run lin",
    "start": "electron ."
  },
  "repository": "https://github.com/jbruwes/kosmos3",
  "keywords": [
    "kosmos3",
    "cms",
    "s3"
  ],
  "author": "Jerry Bruwes",
  "license": "MIT",
  "devDependencies": {
    "svg2png": "^4.1.1",
    "electron-packager": "^15.4.0",
    "electron-icon-builder": "^2.0.1",
    "@babel/core": "^7.17.4",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "acorn": "^8.7.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.1",
    "eslint-plugin-prettier": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdoc": "^3.6.10",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "prettier": "^2.5.1",
    "terser-webpack-plugin": "^5.3.1",
    "util": "^0.12.4",
    "webpack": "^5.69.0",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.4",
    "favicons-webpack-plugin": "^5.0.2",
    "mini-css-extract-plugin": "^2.5.3",
    "css-loader": "^5.2.7"
  },
  "dependencies": {
    "electron": "^17.0.1",
    "electron-serve": "^1.1.0",
    "core-js": "^3.21.0",
    "regenerator-runtime": "^0.13.9",
    "kendo-ui-core": "^2019.1.220",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "fomantic-ui-css": "^2.8.8",
    "hover.css": "^2.3.2",
    "jarallax": "^2.0.1",
    "jquery": "^3.6.0",
    "jsel": "^1.1.6",
    "glightbox": "^3.1.0",
    "lightslider": "^1.1.6",
    "pure": "^2.85.0",
    "webix": "^9.1.0",
    "webix-jet": "^2.1.3",
    "@aws-sdk/client-s3": "^3.51.0",
    "@aws-sdk/client-iam": "^3.51.0",
    "fabric": "^5.0.0",
    "tinymce": "^5.10.3",
    "ace-builds": "^1.4.14",
    "onpushstate": "^0.4.0",
    "@mdi/font": "^6.5.95",
    "@fontsource/arsenal": "^4.5.1",
    "@fontsource/bad-script": "^4.5.1",
    "@fontsource/caveat": "^4.5.3",
    "@fontsource/comfortaa": "^4.5.4",
    "@fontsource/cormorant-garamond": "^4.5.1",
    "@fontsource/cormorant-infant": "^4.5.2",
    "@fontsource/cormorant-sc": "^4.5.2",
    "@fontsource/cormorant-unicase": "^4.5.2",
    "@fontsource/cormorant": "^4.5.1",
    "@fontsource/jura": "^4.5.4",
    "@fontsource/marck-script": "^4.5.1",
    "@fontsource/montserrat": "^4.5.5",
    "@fontsource/montserrat-alternates": "^4.5.1",
    "@fontsource/open-sans-condensed": "^4.5.2",
    "@fontsource/open-sans": "^4.5.5",
    "@fontsource/oswald": "^4.5.4",
    "@fontsource/pattaya": "^4.5.2",
    "@fontsource/poiret-one": "^4.5.1",
    "@fontsource/roboto-condensed": "^4.5.2",
    "@fontsource/roboto-mono": "^4.5.3",
    "@fontsource/roboto-slab": "^4.5.4",
    "@fontsource/roboto": "^4.5.3",
    "@fontsource/rubik-mono-one": "^4.5.1",
    "@fontsource/rubik": "^4.5.3",
    "@fontsource/tenor-sans": "^4.5.1"
  },
  "optionalDependencies": {
    "electron-installer-dmg": "^3.0.0",
    "electron-installer-debian": "^3.1.0",
    "electron-installer-redhat": "^3.3.0",
    "electron-installer-windows": "^3.0.0"
  }
}
