{
  "name": "kosmos3",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "scripts": {
    "format": "prettier -w .",
    "clean:icons": "rimraf icons icon.png",
    "clean:orbita": "rimraf public/orbita",
    "clean": "rimraf dist out",
    "all": "npm run clean && npm run build && npm run icons && npm run build:proton",
    "icons": "npm run clean:icons && .\"/node_modules/.bin/svg2png\" .\"/rocket.svg\" --output=.\"/icon.png\" && .\"/node_modules/.bin/electron-icon-builder\" --flatten --input=.\"/icon.png\" --output=.",
    "build:proton": "webpack --config ./proton/webpack.config.js",
    "build:orbita": "webpack --config ./orbita/webpack.config.js",
    "build:kosmos3": "vue-cli-service build",
    "serve": "vue-cli-service serve",
    "build": "npm run clean:orbita && npm run build:orbita && npm run build:kosmos3",
    "lint": "vue-cli-service lint '**/*.js' '**/*.vue'",
    "darwin": "npx electron-packager dist kosmos3 --platform=darwin --arch=universal --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "win32": "npx electron-packager dist kosmos3 --platform=win32 --arch=x64 --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "linux": "npx electron-packager dist --platform=linux --arch=x64 --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "dmg": "./node_modules/.bin/electron-installer-dmg --overwrite --title=kosmos3 --icon=./icons/icon.icns ./out/kosmos3-darwin-universal/kosmos3.app ./out/kosmos3",
    "deb": "./node_modules/.bin/electron-installer-debian --src out/kosmos3-linux-x64 --dest out --arch amd64",
    "msi": ".\"/node_modules/.bin/electron-installer-windows\" --src \"./out/kosmos3-win32-x64\" --dest out",
    "mac": "npm run darwin && npm run dmg",
    "win": "npm run win32 && npm run msi",
    "lin": "npm run linux && npm run deb",
    "macos-latest": "npm run all && npm run mac",
    "windows-latest": "npm run all && npm run win",
    "ubuntu-latest": "npm run all && npm run lin",
    "start": "electron ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.201.0",
    "@fontsource/arsenal": "^4.5.9",
    "@fontsource/bad-script": "^4.5.8",
    "@fontsource/caveat": "^4.5.9",
    "@fontsource/comfortaa": "^4.5.10",
    "@fontsource/cormorant": "^4.5.9",
    "@fontsource/cormorant-garamond": "^4.5.10",
    "@fontsource/cormorant-infant": "^4.5.11",
    "@fontsource/cormorant-sc": "^4.5.11",
    "@fontsource/cormorant-unicase": "^4.5.11",
    "@fontsource/jura": "^4.5.12",
    "@fontsource/marck-script": "^4.5.11",
    "@fontsource/montserrat": "^4.5.13",
    "@fontsource/montserrat-alternates": "^4.5.11",
    "@fontsource/open-sans": "^4.5.13",
    "@fontsource/open-sans-condensed": "^4.5.5",
    "@fontsource/oswald": "^4.5.11",
    "@fontsource/pattaya": "^4.5.10",
    "@fontsource/poiret-one": "^4.5.11",
    "@fontsource/roboto": "^4.5.8",
    "@fontsource/roboto-condensed": "^4.5.9",
    "@fontsource/roboto-mono": "^4.5.8",
    "@fontsource/roboto-slab": "^4.5.9",
    "@fontsource/rubik": "^4.5.11",
    "@fontsource/rubik-mono-one": "^4.5.10",
    "@fontsource/tenor-sans": "^4.5.10",
    "@mdi/font": "^7.0.96",
    "@tinymce/tinymce-vue": "^5.0.0",
    "@vueuse/core": "^9.4.0",
    "@vueuse/head": "^0.9.8",
    "@vueuse/math": "^9.4.0",
    "ace-builds": "^1.12.5",
    "animate.css": "^4.1.1",
    "core-js": "^3.26.0",
    "dompurify": "^2.4.0",
    "electron": "^21.2.1",
    "electron-serve": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "glightbox": "^3.2.0",
    "jsel": "^1.1.6",
    "konva": "^8.3.13",
    "p5": "^1.5.0",
    "page": "^1.11.6",
    "pinia": "^2.0.23",
    "three": "^0.145.0",
    "tinymce": "^6.2.0",
    "tsparticles": "^2.4.0",
    "tsparticles-demo-configs": "^1.4.0",
    "tsparticles-engine": "^2.4.0",
    "vanta": "^0.5.24",
    "vue": "^3.2.41",
    "vue-konva": "^3.0.1",
    "vue-router": "^4.1.6",
    "vue3-ace-editor": "^2.2.2",
    "vue3-animate-onscroll": "^1.0.6",
    "vue3-particles": "^2.4.0",
    "vue3-runtime-template": "^1.0.2",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/eslint-parser": "^7.19.1",
    "@prettier/plugin-pug": "^2.3.0",
    "@volar/vue-language-plugin-pug": "^1.0.9",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "copy-webpack-plugin": "^11.0.0",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "electron-icon-builder": "^2.0.1",
    "electron-packager": "^16.0.0",
    "eslint": "^8.26.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.7.0",
    "eslint-plugin-vue-pug": "^0.5.4",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "raw-loader": "^4.0.2",
    "svg2png": "^4.1.1",
    "terser-webpack-plugin": "^5.3.6",
    "vue-cli-plugin-pug": "^2.0.0",
    "vue-cli-plugin-vuetify": "^2.5.8",
    "vue-loader": "^17.0.1",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^4.10.0",
    "webpack-plugin-vuetify": "^2.0.0"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^3.1.0",
    "electron-installer-dmg": "^4.0.0",
    "electron-installer-windows": "^3.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "globals": {
      "init": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base",
      "plugin:jsdoc/recommended",
      "plugin:vue/vue3-recommended",
      "plugin:vue-pug/vue3-recommended",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "parser": "vue-eslint-parser",
    "rules": {
      "jsdoc/require-jsdoc": [
        "error",
        {
          "require": {
            "FunctionDeclaration": true,
            "MethodDefinition": true,
            "ClassDeclaration": true,
            "ClassExpression": true,
            "ArrowFunctionExpression": true,
            "FunctionExpression": true
          }
        }
      ]
    },
    "plugins": [
      "jsdoc",
      "vue"
    ],
    "settings": {
      "import/resolver": {
        "alias": {
          "map": [
            [
              "@",
              "./src"
            ]
          ]
        }
      },
      "import/core-modules": [
        "vuetify/styles",
        "vuetify/iconsets/fa",
        "vuetify/iconsets/mdi",
        "vuetify/components",
        "vuetify/directives"
      ]
    }
  },
  "browserslist": [
    "defaults and supports es6-module"
  ]
}
