    "@mdi/font": "^6.7.96",
    "ace-builds": "^1.5.2",
    "aos": "^2.3.4",
    "core-js": "^3.22.7",
    "dompurify": "^2.3.8",
    "electron": "^18.3.1",
    "electron-serve": "^1.1.0",
