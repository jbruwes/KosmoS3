{
  "name": "kosmos3",
  "version": "3.7.6",
  "description": "KosmoS3",
  "main": "main.js",
  "scripts": {
    "format": "prettier -w .",
    "clean:icons": "rimraf icons icon.png",
    "clean": "rimraf dist out && npm run clean:icons",
    "lint": "eslint --fix .",
    "build": "npm run fel && npm run cms",
    "all": "npm run clean && npm run build && npm run icons && npm run pkg",
    "serve": "webpack serve --mode development --open --config ./cms/webpack.config.js",
    "cms": "webpack --env production=true --config ./cms/webpack.config.js",
    "icons": "npm run clean:icons && .\"/node_modules/.bin/svg2png\" .\"/rocket.svg\" --output=.\"/icon.png\" && .\"/node_modules/.bin/electron-icon-builder\" --flatten --input=.\"/icon.png\" --output=.",
    "fel": "webpack --config ./fel/webpack.config.js",
    "pkg": "webpack --config ./pkg/webpack.config.js",
    "fdocs": "jsdoc -c ./.jsdoc.conf.fel.json",
    "cdocs": "jsdoc -c ./.jsdoc.conf.cms.json",
    "darwin": "npx electron-packager dist KosmoS3 --platform=darwin --arch=universal --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "win32": "npx electron-packager dist KosmoS3 --platform=win32 --arch=x64 --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "linux": "npx electron-packager dist --platform=linux --arch=x64 --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "dmg": "./node_modules/.bin/electron-installer-dmg --overwrite --title=KosmoS3 --icon=./icons/icon.icns ./out/kosmos3-darwin-universal/KosmoS3.app ./out/kosmos3",
    "deb": "./node_modules/.bin/electron-installer-debian --src out/kosmos3-linux-x64 --dest out --arch amd64",
    "msi": ".\"/node_modules/.bin/electron-installer-windows\" --src \"./out/KosmoS3-win32-x64\" --dest out",
    "mac": "npm run darwin && npm run dmg",
    "win": "npm run win32 && npm run msi",
    "lin": "npm run linux && npm run deb",
    "macos-latest": "npm run all && npm run mac",
    "windows-latest": "npm run all && npm run win",
    "ubuntu-latest": "npm run all && npm run lin",
    "start": "electron ."
  },
  "repository": "https://github.com/jbruwes/kosmos3",
  "keywords": [
    "kosmos3",
    "cms",
    "s3"
  ],
  "author": "Jerry Bruwes",
  "license": "GNU AGPL",
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/aos": "^3.0.4",
    "@types/dompurify": "^2.3.3",
    "@types/jquery": "^3.5.14",
    "@types/node": "^17.0.36",
    "@types/page": "^1.11.5",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^5.2.7",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "electron-icon-builder": "^2.0.1",
    "electron-packager": "^15.5.1",
    "eslint": "^8.16.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-jsdoc": "^39.3.2",
    "eslint-plugin-prettier": "^4.0.0",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "glob": "7.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jsdoc": "^3.6.10",
    "jsdoc-vuejs": "^4.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "svg2png": "^4.1.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.7.2",
    "vue-loader": "^17.0.0",
    "webpack": "^5.72.1",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.100.0",
