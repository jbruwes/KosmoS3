{
  "name": "kosmos3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docs:orbita": "jsdoc -c ./orbita/.jsdoc.conf.json",
    "docs:kosmos3": "jsdoc -c ./.jsdoc.conf.json",
    "format": "prettier -w .",
    "clean:icons": "rimraf icons icon.png",
    "clean:orbita": "rimraf public/orbita",
    "clean": "rimraf dist out",
    "all": "npm run clean && npm run build && npm run icons && npm run build:proton",
    "icons": "npm run clean:icons && .\"/node_modules/.bin/svg2png\" .\"/rocket.svg\" --output=.\"/icon.png\" && .\"/node_modules/.bin/electron-icon-builder\" --flatten --input=.\"/icon.png\" --output=.",
    "build:proton": "webpack --config ./proton/webpack.config.js",
    "build:orbita": "webpack --config ./orbita/webpack.config.js",
    "build:kosmos3": "vite build",
    "dev": "vite",
    "build": "npm run clean:orbita && npm run build:orbita && npm run build:kosmos3",
    "preview": "vite preview",
    "lint": "eslint . --ext .js,.vue --fix --ignore-path .gitignore",
    "darwin": "npx electron-packager dist kosmos3 --platform=darwin --arch=universal --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "win32": "npx electron-packager dist kosmos3 --platform=win32 --arch=x64 --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "linux": "npx electron-packager dist --platform=linux --arch=x64 --overwrite --prune=true --asar --out=out --icon=./icons/icon",
    "dmg": "./node_modules/.bin/electron-installer-dmg --overwrite --title=kosmos3 --icon=./icons/icon.icns ./out/kosmos3-darwin-universal/kosmos3.app ./out/kosmos3",
    "deb": "./node_modules/.bin/electron-installer-debian --src out/kosmos3-linux-x64 --dest out --arch amd64",
    "msi": ".\"/node_modules/.bin/electron-installer-windows\" --src \"./out/kosmos3-win32-x64\" --dest out",
    "mac": "npm run darwin && npm run dmg",
    "win": "npm run win32 && npm run msi",
    "lin": "npm run linux && npm run deb",
    "macos-latest": "npm run all && npm run mac",
    "windows-latest": "npm run all && npm run win",
    "ubuntu-latest": "npm run all && npm run lin",
    "start": "electron ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.341.0",
    "@fontsource/arsenal": "^5.0.2",
    "@fontsource/bad-script": "^5.0.2",
    "@fontsource/caveat": "^5.0.2",
    "@fontsource/comfortaa": "^5.0.2",
    "@fontsource/cormorant": "^5.0.2",
    "@fontsource/cormorant-garamond": "^5.0.2",
    "@fontsource/cormorant-infant": "^5.0.2",
    "@fontsource/cormorant-sc": "^5.0.2",
    "@fontsource/cormorant-unicase": "^5.0.2",
    "@fontsource/jura": "^5.0.2",
    "@fontsource/marck-script": "^5.0.2",
    "@fontsource/montserrat": "^5.0.2",
    "@fontsource/montserrat-alternates": "^5.0.2",
    "@fontsource/open-sans": "^5.0.2",
    "@fontsource/open-sans-condensed": "^4.5.5",
    "@fontsource/oswald": "^5.0.2",
    "@fontsource/pattaya": "^5.0.2",
    "@fontsource/poiret-one": "^5.0.2",
    "@fontsource/roboto": "^5.0.2",
    "@fontsource/roboto-condensed": "^5.0.2",
    "@fontsource/roboto-mono": "^5.0.2",
    "@fontsource/roboto-slab": "^5.0.2",
    "@fontsource/rubik": "^5.0.2",
    "@fontsource/rubik-mono-one": "^5.0.2",
    "@fontsource/tenor-sans": "^5.0.2",
    "@mdi/font": "^7.2.96",
    "@tinymce/tinymce-vue": "^5.1.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vueuse/core": "^10.1.2",
    "@vueuse/head": "^1.1.26",
    "@vueuse/math": "^10.1.2",
    "ace-builds": "^1.22.0",
    "animate.css": "^4.1.1",
    "core-js": "^3.30.2",
    "dompurify": "^3.0.3",
    "electron": "^24.4.0",
    "electron-serve": "^1.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "glightbox": "^3.2.0",
    "jsel": "^1.1.6",
    "p5": "^1.6.0",
    "page": "^1.11.6",
    "pinia": "^2.1.3",
    "terser-webpack-plugin": "^5.3.9",
    "three": "^0.152.2",
    "tinymce": "^6.4.2",
    "tsparticles": "^2.9.3",
    "tsparticles-demo-configs": "^2.9.3",
    "tsparticles-engine": "^2.9.3",
    "vanta": "^0.5.24",
    "vite": "^4.3.9",
    "vite-plugin-vuetify": "^1.0.2",
    "vue": "^3.3.4",
    "vue-konva": "^3.0.2",
    "vue-router": "^4.2.2",
    "vue3-ace-editor": "^2.2.2",
    "vue3-animate-onscroll": "^1.0.6",
    "vue3-particles": "^2.9.3",
    "vue3-runtime-template": "^1.0.2",
    "vuedraggable": "^4.1.0",
    "vuetify": "^3.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-jsdoc": "^45.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.14.1",
    "eslint-plugin-vue-pug": "^0.6.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.6",
    "pug": "^3.0.2",
    "rimraf": "^5.0.1",
    "sass": "^1.62.1",
    "vue-loader": "^17.1.2",
    "webpack-assets-manifest": "^5.1.0",
    "webpack-cli": "^5.1.1"
  },
  "optionalDependencies": {
    "electron-installer-debian": "^3.1.0",
    "electron-installer-dmg": "^4.0.0",
    "electron-installer-windows": "^3.0.0"
  }
}
